      <Query>
        <DataSourceName>NflCombine</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT p.ParticipantId, p.Firstname + ' ' + p.Lastname as playerName, poscat.name AS position_category, MIN(forty.result) AS [40-yd], MAX(bench.result) AS bench, MAX(vert.result) AS vert, MAX(broad.result) AS broad, MIN(three.result) AS [3-cone], MIN(twenty.result) AS [20-yd-shuttle], MIN(sixty.result) AS [60-yd-shuttle]
FROM   dbo.Participants AS p LEFT OUTER JOIN
             dbo.Colleges AS c ON p.CollegeId = c.CollegeId LEFT OUTER JOIN
             dbo.Conferences AS conf ON c.ConferenceId = conf.ConferenceId LEFT OUTER JOIN
             dbo.Positions AS pos ON p.PositionId = pos.PositionId LEFT OUTER JOIN
             dbo.PositionCategories AS poscat ON pos.CategoryId = poscat.PositionCategoryId LEFT OUTER JOIN
             dbo.WorkoutResults AS forty ON p.ParticipantId = forty.ParticipantId LEFT OUTER JOIN
             dbo.WorkoutResults AS bench ON p.ParticipantId = bench.ParticipantId LEFT OUTER JOIN
             dbo.WorkoutResults AS vert ON p.ParticipantId = vert.ParticipantId LEFT OUTER JOIN
             dbo.WorkoutResults AS broad ON p.ParticipantId = broad.ParticipantId LEFT OUTER JOIN
             dbo.WorkoutResults AS three ON p.ParticipantId = three.ParticipantId LEFT OUTER JOIN
             dbo.WorkoutResults AS twenty ON p.ParticipantId = twenty.ParticipantId LEFT OUTER JOIN
             dbo.WorkoutResults AS sixty ON p.ParticipantId = sixty.ParticipantId
WHERE (forty.WorkoutId = 1 OR
             forty.WorkoutId IS NULL) AND (bench.WorkoutId = 2 OR
             bench.WorkoutId IS NULL) AND (vert.WorkoutId = 3 OR
             vert.WorkoutId IS NULL) AND (broad.WorkoutId = 4 OR
             broad.WorkoutId IS NULL) AND (three.WorkoutId = 5 OR
             three.WorkoutId IS NULL) AND (twenty.WorkoutId = 6 OR
             twenty.WorkoutId IS NULL) AND (sixty.WorkoutId = 7 OR
             sixty.WorkoutId IS NULL) and poscat.Name = @ParamPosition
GROUP BY p.ParticipantId, p.firstname, p.lastname, p.height, p.weight, p.hands, p.overview, p.strengths, p.weaknesses, p.comparision, p.BottomLine, p.WhatScoutsSay, p.ExpertGrade, p.link, p.pick, pos.name, c.name, poscat.name, conf.name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ParamPosition">
            <Value>=Parameters!ParamPosition.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
