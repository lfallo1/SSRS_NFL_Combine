  <DataSources>
    <DataSource Name="NflCombine">
      <DataSourceReference>NflCombine</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>13981cf0-5c3b-453b-9188-01c038db25f0</rd:DataSourceID>
